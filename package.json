    "fs-extra": "^11.2.0",
    "is-number": "^7.0.0",
    "next": "14.1.4",
    "next-auth-steam": "^0.3.0",
    "next-mdx-remote": "^4.4.1",
    "node-fetch": "^3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-query": "^3.39.3",
    "sharp": "^0.33.3",
    "superjson": "^2.2.1",
